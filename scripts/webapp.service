[Unit]
Description=webapp

[Service]
EnvironmentFile=/etc/environment
User=ubuntu
ExecStart=/usr/bin/java -jar /home/ubuntu/webapp/cloudwebapp-0.0.1-SNAPSHOT.jar >> /home/ubuntu/webapp.log 2 >> /home/ubuntu/webapp.log
SuccessExitStatus=143
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target